             <TextArea fx:id="textArea" editable="false" prefHeight="110.0" prefWidth="400.0"
                       AnchorPane.topAnchor="0.0">
                  <font>
                      <Font size="16.0" />
                  </font>
              </TextArea>
             <Button fx:id="btnTerminarTurno" layoutX="50.0" layoutY="109.0" mnemonicParsing="false" prefWidth="300"
                     text="Terminar turno" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="50.0"
                     AnchorPane.rightAnchor="50.0"/>
         </children>
       </AnchorPane>
   </right>
   <bottom>
      <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
