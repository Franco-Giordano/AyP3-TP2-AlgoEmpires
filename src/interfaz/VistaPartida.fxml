<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.*?>
<BorderPane fx:id="panePadre" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="interfaz.VistaPartidaController">
    <center>
        <GridPane fx:id="pane">
        </GridPane>
    </center>
    <right>
        <VBox focusTraversable="false" spacing="5.0" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="btnTerminarTurno" focusTraversable="false" layoutX="50.0" layoutY="109.0" mnemonicParsing="false" prefWidth="300" text="Terminar turno" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" />
                <TextArea fx:id="textArea" focusTraversable="false" prefHeight="50.0" prefWidth="300.0" wrapText="true"
                          AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="0.0"/>
            </children>
        </VBox>
    </right>
    <bottom>
        <HBox focusTraversable="false" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
    </bottom>
</BorderPane>
