<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox focusTraversable="false" BorderPane.alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="interfaz.botoneras.BotoneraArmaDeAsedioController">
    <children>
        <Button focusTraversable="false" mnemonicParsing="false" onAction="#atacar" prefHeight="60.0" prefWidth="300.0" style="-fx-background-image: url('/interfaz/recursos/imagenes/fondoBoton.png'); -fx-background-size: stretch; -fx-background-radius: 20 20 20 20;" text="Atacar" textFill="WHITE">
            <font>
                <Font size="20.0" />
            </font>
        </Button>
        <Button focusTraversable="false" mnemonicParsing="false" onAction="#desmontar" prefHeight="60.0" prefWidth="300.0" style="-fx-background-image: url('/interfaz/recursos/imagenes/fondoBoton.png'); -fx-background-size: stretch; -fx-background-radius: 20 20 20 20;" text="Desmontar" textFill="WHITE">
            <font>
                <Font size="20.0" />
            </font>
        </Button>
    </children>
</VBox>
