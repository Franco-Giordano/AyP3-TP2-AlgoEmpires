<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" focusTraversable="false" scaleX="0.8" scaleY="0.8" spacing="10.0"
      BorderPane.alignment="CENTER" xmlns="http://javafx.com/javafx/8"
      fx:controller="interfaz.botoneras.BotoneraEdificioController">
    <children>
        <Button focusTraversable="false" mnemonicParsing="false" onAction="#crearArmaDeAsedio" prefHeight="60.0"
                prefWidth="300.0" stylesheets="/interfaz/botoneras/estiloBoton.css" text="Crear Arma De Asedio"
                textFill="WHITE">
            <font>
                <Font name="Liberation Serif" size="26.0"/>
            </font>
        </Button>
    </children>
</VBox>
